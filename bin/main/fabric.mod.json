{
  "schemaVersion": 1,
  "id": "stuff",
  "version": "${version}",
  "name": "Stuff",
  "description": "Your mod description",
  "authors": ["Your Name"],
  "contact": {
    "homepage": "Your mod homepage",
    "sources": "Your mod source repository"
  },
  "license": "All-Rights-Reserved",
  "icon": "assets/stuff/icon.png",
  "environment": "*",
  "entrypoints": {
    "client": [
      "org.simon.stuff.client.StuffClient"
    ],
    "main": [
      "org.simon.stuff.Stuff"
    ],
    "fabric-datagen": [
      "org.simon.stuff.StuffDataGenerator"
    ]
  },
  "mixins": [
    "stuff.mixins.json",
    {
      "config": "stuff.client.mixins.json",
      "environment": "client"
    }
  ],
  "depends": {
    "fabricloader": ">=${loader_version}",
    "fabric": "*",
    "minecraft": "${minecraft_version}",
    "geckolib": ">=${geckolib_version}",
    "fabwork": "*"
  },
  "suggests": {
    "modmenu": "*"
  },
  "custom": {
    "fabwork": {
      "requiredOn": "*"
    }
  }
}
